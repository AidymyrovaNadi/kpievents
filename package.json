{
  "name": "kpievents",
  "version": "1.0.0",
  "description": "KPIEvents is calendar that structures all events in KPI.",
  "engines": {
    "node": "12.x"
  },
  "private": true,
  "workspaces": ["telegram-bot", "server", "client"],
  "scripts": {
    "start": "start npm run client && start npm run server && start npm run bot",
    "build": "cd client && npm run build",
    "server": "node server",
    "bot": "node telegram-bot",
    "client": "cd client && npm run start",
    "test": "eslint **/*.js",
    "fix": "eslint **/*.js --fix"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules",
    "server/node_modules",
    "telegram-bot/node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AidymyrovaNadi/kpievents.git"
  },
  "keywords": [
    "KPI"
  ],
  "author": "Kishko Igor, Aidymyrova Nadi, Borshosh Sasha",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/AidymyrovaNadi/kpievents/issues"
  },
  "devDependencies": {
    "eslint": "^6.6.0"
  },
  "dependencies": {}
}
